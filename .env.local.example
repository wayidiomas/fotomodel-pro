# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Google Banana API (para geração de imagens)
GOOGLE_BANANA_API_KEY=your-google-banana-api-key

# Stripe Configuration
# Get your keys from: https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY_TEST=sk_test_your_test_key_here
STRIPE_SECRET_KEY_LIVE=sk_live_your_live_key_here
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY_TEST=pk_test_your_test_key_here
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY_LIVE=pk_live_your_live_key_here

# Stripe Webhook Secret
# Get this from: https://dashboard.stripe.com/webhooks
STRIPE_WEBHOOK_SECRET_TEST=whsec_your_test_webhook_secret
STRIPE_WEBHOOK_SECRET_LIVE=whsec_your_live_webhook_secret

# Stripe Environment (test or live)
STRIPE_ENVIRONMENT=test

# Admin Secret (para endpoints protegidos como sync-products)
ADMIN_SECRET=your_secure_random_secret_here
